generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["relationJoins", "omitApi"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}



model User {
  id String @id
  username String @unique
  firstName String
  lastName String
  isAdmin Boolean
  password String
  createdAt DateTime
  updatedAt DateTime
}